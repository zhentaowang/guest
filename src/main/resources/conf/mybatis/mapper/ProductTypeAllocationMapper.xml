<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhiweicloud.guest.mapper.ProductTypeAllocationMapper">
  <resultMap id="BaseResultMap" type="com.zhiweicloud.guest.model.ProductTypeAllocation">
    <id column="id" jdbcType="BIGINT" property="id" />
    <result column="category_no" jdbcType="VARCHAR" property="categoryNo" />
    <result column="product_category" jdbcType="VARCHAR" property="productCategory" />
    <result column="service_type" jdbcType="VARCHAR" property="serviceType" />
    <result column="free_retinue_num" jdbcType="INTEGER" property="freeRetinueNum" />
    <result column="remark" jdbcType="VARCHAR" property="remark" />
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
    <result column="update_time" jdbcType="TIMESTAMP" property="updateTime" />
    <result column="is_deleted" jdbcType="SMALLINT" property="isDeleted" />
  </resultMap>

  <resultMap id="DropdownlistMap" type="com.zhiweicloud.guest.model.Dropdownlist">
    <id column="id" jdbcType="BIGINT" property="id" />
    <result column="value" jdbcType="VARCHAR" property="value" />
  </resultMap>

  <select id="getProductTypeDropdownList" resultMap="DropdownlistMap">
    select id,product_category as value from product_type_allocation where is_deleted = 0
  </select>
</mapper>